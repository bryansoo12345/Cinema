@model IEnumerable<CinemaBranch>
@{
    ViewData["Title"] = "Cinema";
}

<h1>@ViewData["Title"] Branch</h1>
<br />
<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Mall</th>
            <th>City</th>
            <th>State</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td width="50%">@obj.MallName</td>
                <td width="25%">@obj.City</td>
                <td width="25%">@obj.Sate</td>
            </tr>
        }
    </tbody>
</table>