            <div class="tab-pane fade active show" id="home" role="tabpanel">
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th class="text-center" scope="col">Date</th>
                                <th scope="col">Speakers</th>
                                <th scope="col">Session</th>
                                <th scope="col">Venue</th>
                                <th class="text-center" scope="col"></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var movies in Model.MovieShows)
                            {
                                <tr class="inner-box show-list">
                                    <th scope="row">
                                        <div class="event-date">
                                            <span>16</span>
                                            <p>Novembar</p>
                                        </div>
                                    </th>
                                    <td>
                                        <div class="event-img">
                                            @if (movies.Movie.PhotoFile != null && movies.Movie.PhotoFile.Length > 0)
                                            {
                                                var imageBase64 = Convert.ToBase64String(movies.Movie.PhotoFile);
                                                <img src="data:image;base64,@imageBase64" alt="Mall Photo" class="img-fluid" style="width: 370px; height: 550px; margin-top:10px; margin-bottom:10px; "/>
                                            }
                                        </div>
                                    </td>
                                    <td>
                                        <div class="event-wrap">
                                            <h3>@movies.Movie.Name</h3>
                                            <div class="meta">
                                                <div class="organizers">
                                                    @movies.Movie.Description
                                                </div>
                                                <div class="categories">
                                                    @movies.Movie.Genre
                                                </div>
                                                <div class="time">
                                                    @foreach (var showtime in movies.MovieShowTimes)
                                                    {
                                                        <a class="btn btn-info" asp-area="" asp-controller="Movie" asp-action="BookMovie"  asp-route-ShowCode="@showtime.MovieShowTimeCode" asp-route-HallCode="@showtime.HallCode" asp-route-MovieName="@movies.Movie.Name" style="margin: 5px;">@showtime.StartTime.ToString("h:mm tt") - @showtime.EndTime.ToString("h:mm tt")</a>
                                                    }
                                                </div>


                                            </div>
                                        </div>
                                    </td>
                                    <td>

                                    </td>
                                    <td>
                                        <div class="primary-btn">
                                            <a class="btn btn-primary" href="#">Read More</a>
                                        </div>
                                    </td>
                                </tr>
                            }


                        </tbody>
                    </table>
